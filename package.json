{
  "name": "org.webosbrew.safeupdate",
  "version": "0.1.2",
  "description": "Homebrew Channel Safe Updater",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "package": "ares-package -n -o dist/ build/",
    "deploy": "ares-install dist/${npm_package_name}_${npm_package_version}_all.ipk",
    "launch": "ares-launch ${npm_package_name}",
    "build-manifest": "webosbrew-gen-manifest -a build/appinfo.json -p dist/${npm_package_name}_${npm_package_version}_all.ipk  -i assets/icon.png -o dist/${npm_package_name}.manifest.json -l https://github.com/webosbrew/homebrew-channel-safe-updater"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.14.0",
    "regenerator-runtime": "^0.13.7",
    "webostvjs": "^1.2.5",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@webosbrew/pub-utils": "^1.1.0",
    "@webosose/ares-cli": "^2.2.0",
    "add-asset-webpack-plugin": "^2.0.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.7.1"
  }
}
